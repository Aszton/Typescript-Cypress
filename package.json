    "dev": "npx cypress run --browser chrome --config baseUrl=https://demo.saleor.io/dashboard/",
    "test": "npx cypress run --browser chrome --config baseUrl=https://demo.saleor.io/dashboard/",
    "preProd": "npx cypress run --browser chrome --config baseUrl=https://demo.saleor.io/dashboard/",
    "prod": "npx cypress run --browser chrome --config baseUrl=https://demo.saleor.io/dashboard/",
